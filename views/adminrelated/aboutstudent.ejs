<%- include("../adminlayout/admin_header"); -%>

    <table class="table table-striped alignmiddle">
        <thead>
            <tr>
                <th>Roll-Number</th>
                <th>student-name</th>
                <th>result upload</th>
            </tr>
        </thead>
        <% if(typeof selected_student!=undefined) {%>
            <tbody>
                <% selected_student.forEach((e,j)=>{%>
                    <tr>
                        <td>
                            <%=j%>
                        </td>
                        <td>
                            <%=e.name%>
                        </td>
                        <td>
                            <a class="btn btn-danger" href="/result/markfillup/<%=e.userId%> ">Add</a>
                        </td>
                        <td>
                            <a class="btn btn-danger" href="/result/markfillup/<%=e.userId%> ">Delete</a>
                        </td>


                    </tr>
                    <%}) %>
            </tbody>
            <% } %>
    </table>

    <%- include("../adminlayout/admin_footer"); -%>