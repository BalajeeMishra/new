<%- include("../adminlayout/admin_header"); -%>

    <table class="table table-striped alignmiddle">
        <thead>
            <tr>
                <th>Roll-Number</th>
                <th>student-name</th>
                <th>result upload</th>
            </tr>
        </thead>
        <% if(selected_student) {%>
            <tbody>

                <% selected_student.forEach((e,j)=>{%>
                    <tr>
                        <td>
                            <%=j%>
                        </td>
                        <td>
                            <%=e.name%>
                        </td>
                        <%=bool %>
                            <% if(bool){ %>
                                <td>
                                    <a class="btn btn-success" href="#">Uploaded</a>
                                </td>
                                <% } else{ %>

                                    <td>
                                        <a class="btn btn-danger" href="/result/markfillup/<%=e.userId%> ">Add</a>
                                    </td>
                                    <% } %>
                                        <td>
                                            <a class="btn btn-danger"
                                                href="/result/markfillup/<%=e.userId%> ">Delete</a>
                                        </td>


                    </tr>
                    <%}) %>
            </tbody>
            <% } %>
    </table>

    <%- include("../adminlayout/admin_footer"); -%>